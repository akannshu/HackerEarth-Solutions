#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    
    while(t--)
    {
        string s1;
        cin>>s1;
        string s2 = s1;
        
        reverse(s2.begin(),s2.end());
        
        if(s1.compare(s2)==0 and s1.length()%2==0)
            cout<<"YES"<<" "<<"EVEN"<<endl;

        else if(s1.compare(s2)==0 and s1.length()%2!=0)
            cout<<"YES"<<" "<<"ODD"<<endl;
        
        else
            cout<<"NO"<<endl;
    }
    
    return 0;
}
